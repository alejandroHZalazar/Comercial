                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        lpad(o.id, 8, 0) AS id, o.fecha, o.fk_proveedor, o.total, o.procesado, o.iva, o.recargo, o.descuento, d.fk_producto, d.codBarras, d.codProveedor, d.descripcion, d.precioProveedor,d.cantidad, d.subtotal, p.imagen, 
